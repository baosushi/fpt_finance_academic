@using CaptstoneProject.Areas.AdminTrainingDepartment.Controllers
@model List<CaptstoneProject.Areas.AdminTrainingDepartment.Controllers.AdminTrainingController.MyData>


@{
    Layout = null;
}
<h2>GetMyData</h2>
<table id="mTable">
    <thead>
        <tr>
            <td>No.</td>
            <th>
                CourseName
            </th>
            <th>ClassName</th>
            <th>SubjectName</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>@i</td>
                <td>@Model[i - 1].CourseName</td>
                <td>@Model[i - 1].ClassName</td>
                <td>@Model[i - 1].SubjectName</td>

            </tr>

        }
    </tbody>
</table>
<script>
    $("#mTable").DataTable({
        "searching": true
    });
</script>
